<?xml version="1.0" encoding="utf-8"?>
<VisualStudioToolFile
	Name="Ase refinement"
	Version="8,00"
	>
	<Rules>
		<CustomBuildRule
			Name="ase"
			DisplayName="ase"
			CommandLine="&quot;$(SolutionDir)tools\AseParse&quot; [inputs] scene.ase [PathPrepend]"
			Outputs="scene.ase"
			FileExtensions="*.ase"
			ExecutionDescription="Changing textures path to [PathPrepend] in sceneRaw.ase (output to scene.ase)..."
			>
			<Properties>
				<StringProperty
					Name="PathPrepend"
					DisplayName="PathPrepend"
					DefaultValue="textures/"
				/>
			</Properties>
		</CustomBuildRule>
	</Rules>
</VisualStudioToolFile>
